{
    "id": "/{{app_name}}",
    "instances": 1,
    "cpus": 0.25,
    "mem": 32,
    "cmd": "python3 -m http.server 8080",
    "container": {
        "type": "DOCKER",
        "docker": {
            "image": "python:3",
            "privileged": true,
            "network": "BRIDGE",
            "portMappings": [
                {
                    "containerPort": 8080,
                    "hostPort": 0
                }
                ]
        }
    },
    "healthChecks": [
        {
            "protocol": "HTTP",
            "portIndex": 0,
            "path": "/",
            "gracePeriodSeconds": 5,
            "intervalSeconds": 20,
            "maxConsecutiveFailures": 3
        }
    ]
}